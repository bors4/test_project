  "dependencies": {
    "axios": "^1.13.2",
    "chai": "^6.2.1"
  },
  "scripts": {
    "test": "cross-env HEADLESS=true RECORD_VIDEO=false DEBUG_MODE=false wdio run ./wdio.conf.js",
    "test:debug": "cross-env HEADLESS=false RECORD_VIDEO=false DEBUG_MODE=true wdio run ./wdio.conf.js",
    "test:video": "cross-env HEADLESS=true RECORD_VIDEO=true DEBUG_MODE=false wdio run ./wdio.conf.js",
    "test:debug:video": "cross-env HEADLESS=false RECORD_VIDEO=true DEBUG_MODE=true wdio run ./wdio.conf.js",
    "test:feature": "cross-env HEADLESS=false RECORD_VIDEO=true wdio run ./wdio.conf.js --spec",
    "test:tags": "cross-env HEADLESS=false RECORD_VIDEO=true wdio run ./wdio.conf.js --spec",
    "format": "prettier --write ./",
    "lint": "eslint ./src",
    "lint:write": "eslint --debug src/ --fix",
    "prettier": "prettier --write src/**/*.js",
    "allure:watch": "npx allure watch allure-results",
    "allure:generate": "npx allure generate allure-results -o allure-report",
    "allure:open": "npx allure open allure-report/awesome",
    "allure:history": "npx allure history allure-results -o allure-report",
    "allure:generate:history": "if exist allure-report\\history (xcopy /E /I allure-report\\history allure-results\\history /Y) & npx allure generate allure-results -o allure-report --report-name=\"Allure Report\""
  }
}
